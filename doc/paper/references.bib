	author = {To, K. K. W. and Yip, C. C. Y. and Lai, C. Y. W. and Wong, C. K. H. and Ho, D. T. Y. and Pang, P. K. P. and Ng, A. C. K. and Leung, K. -H. and Poon, R. W. S. and Chan, K. -H. and Cheng, V. C. C. and Hung, I. F. N. and Yuen, K. -Y.},
	year = {2019},
	keywords = {Influenza virus, Automated, Cost, Nasopharyngeal, Point-of-care testing, Respiratory syncytial virus, Saliva},
	pages = {372--378},
	file = {ScienceDirect Full Text PDF:/Users/nb22i345/Zotero/storage/5WUXG5ES/To et al. - 2019 - Saliva as a diagnostic specimen for testing respir.pdf:application/pdf},
