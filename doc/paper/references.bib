	journal = {BMJ Open},
	author = {Ulyte, Agne and Radtke, Thomas and Abela, Irene Alma and Haile, Sarah R. and Blankenberger, Jacob and Jung, Ruedi and Capelli, Celine and Berger, Christoph and Frei, Anja and Huber, Michael and Schanz, Merle and Schwarzmueller, Magdalena and Trkola, Alexandra and Fehr, Jan and Puhan, Milo Alan and Kriemler, Susi},
	year = {2021},
	keywords = {epidemiology, infection control, community child health, paediatric infectious disease \& immunisation, virology},
	pages = {e047483},
	file = {Full Text PDF:/Users/nb22i345/Zotero/storage/6PFMEG6V/Ulyte et al. - 2021 - Variation in SARS-CoV-2 seroprevalence across dist.pdf:application/pdf;Snapshot:/Users/nb22i345/Zotero/storage/IF7TNLN8/e047483.html:text/html},
}

@article{Styczynski2022,
	title = {Assessing impact of ventilation on airborne transmission of {SARS}-{CoV}-2: a cross-sectional analysis of naturally ventilated healthcare settings in {Bangladesh}},
	volume = {12},
	copyright = {© Author(s) (or their employer(s)) 2022. Re-use permitted under CC BY-NC. No commercial re-use. See rights and permissions. Published by BMJ.. http://creativecommons.org/licenses/by-nc/4.0/This is an open access article distributed in accordance with the Creative Commons Attribution Non Commercial (CC BY-NC 4.0) license, which permits others to distribute, remix, adapt, build upon this work non-commercially, and license their derivative works on different terms, provided the original work is properly cited, appropriate credit is given, any changes made indicated, and the use is non-commercial. See: http://creativecommons.org/licenses/by-nc/4.0/.},
	issn = {2044-6055, 2044-6055},
	shorttitle = {Assessing impact of ventilation on airborne transmission of {SARS}-{CoV}-2},
	url = {https://bmjopen.bmj.com/content/12/4/e055206},
	doi = {10.1136/bmjopen-2021-055206},
	abstract = {Objectives To evaluate the risk of exposure to SARS-CoV-2 in naturally ventilated hospital settings by measuring parameters of ventilation and comparing these findings with results of bioaerosol sampling.
Study design Cross-sectional study.
Study setting and study sample The study sample included nine hospitals in Dhaka, Bangladesh. Ventilation characteristics and air samples were collected from 86 healthcare spaces during October 2020 to February 2021.
Primary outcome Risk of cumulative SARS-CoV-2 infection by type of healthcare area.
Secondary outcomes Ventilation rates by healthcare space; risk of airborne detection of SARS-CoV-2 across healthcare spaces; impact of room characteristics on absolute ventilation; SARS-CoV-2 detection by naturally ventilated versus mechanically ventilated spaces.
Results The majority (78.7\%) of naturally ventilated patient care rooms had ventilation rates that fell short of the recommended ventilation rate of 60 L/s/p. Using a modified Wells-Riley equation and local COVID-19 case numbers, we found that over a 40-hour exposure period, outpatient departments posed the highest median risk for infection (7.7\%). SARS-CoV-2 RNA was most frequently detected in air samples from non-COVID wards (50.0\%) followed by outpatient departments (42.9\%). Naturally ventilated spaces (22.6\%) had higher rates of SARS-CoV-2 detection compared with mechanically ventilated spaces (8.3\%), though the difference was not statistically significant (p=0.128). In multivariable linear regression with calculated elasticity, open door area and cross-ventilation were found to have a significant impact on ventilation.
Conclusion Our findings provide evidence that naturally ventilated healthcare settings may pose a high risk for exposure to SARS-CoV-2, particularly among non-COVID-designated spaces, but improving parameters of ventilation can mitigate this risk.},
