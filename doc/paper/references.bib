@article{Milton2013PLoSPathogens,
	title = {Influenza virus aerosols in human exhaled breath: {Particle} size, culturability, and effect of surgical masks},
	volume = {9},
	issn = {1553-7374},
	shorttitle = {Influenza {Virus} {Aerosols} in {Human} {Exhaled} {Breath}},
	doi = {10.1371/journal.ppat.1003205},
	abstract = {The CDC recommends that healthcare settings provide influenza patients with facemasks as a means of reducing transmission to staff and other patients, and a recent report suggested that surgical masks can capture influenza virus in large droplet spray. However, there is minimal data on influenza virus aerosol shedding, the infectiousness of exhaled aerosols, and none on the impact of facemasks on viral aerosol shedding from patients with seasonal influenza. We collected samples of exhaled particles (one with and one without a facemask) in two size fractions (“coarse”{\textgreater}5 µm, “fine”≤5 µm) from 37 volunteers within 5 days of seasonal influenza onset, measured viral copy number using quantitative RT-PCR, and tested the fine-particle fraction for culturable virus. Fine particles contained 8.8 (95\% CI 4.1 to 19) fold more viral copies than did coarse particles. Surgical masks reduced viral copy numbers in the fine fraction by 2.8 fold (95\% CI 1.5 to 5.2) and in the coarse fraction by 25 fold (95\% CI 3.5 to 180). Overall, masks produced a 3.4 fold (95\% CI 1.8 to 6.3) reduction in viral aerosol shedding. Correlations between nasopharyngeal swab and the aerosol fraction copy numbers were weak (r = 0.17, coarse; r = 0.29, fine fraction). Copy numbers in exhaled breath declined rapidly with day after onset of illness. Two subjects with the highest copy numbers gave culture positive fine particle samples. Surgical masks worn by patients reduce aerosols shedding of virus. The abundance of viral copies in fine particle aerosols and evidence for their infectiousness suggests an important role in seasonal influenza transmission. Monitoring exhaled virus aerosols will be important for validation of experimental transmission studies in humans.},
	language = {en},
	number = {3},
	urldate = {2023-07-19},
	journal = {PLOS Pathogens},
	author = {Milton, Donald K. and Fabian, M. Patricia and Cowling, Benjamin J. and Grantham, Michael L. and McDevitt, James J.},
	year = {2013},
	keywords = {Aerosols, Coughing, Infectious disease control, Influenza, Influenza A virus, Influenza viruses, Respiratory infections, Surgical and invasive medical procedures},
	pages = {e1003205},
	file = {Milton et al_2013_Influenza Virus Aerosols in Human Exhaled Breath.pdf:/Users/nb22i345/Zotero/storage/4IAGIUYU/Milton et al_2013_Influenza Virus Aerosols in Human Exhaled Breath.pdf:application/pdf},
}

